> level0 <- Lgdp[1]
> slope0 <- mean(diff(Lgdp))
> buildGap <- function(u) {
    trend <- dlmModPoly(dV = 1e-7, dW = exp(u[1:2]),
                        m0 = c(level0, slope0), C0 = 2 * diag(2))
    gap <- dlmModARMA(ar = u[4:5], sigma2 = exp(u[3]))
    return(trend + gap)}
> init <- c(-3, -1, -3, .4, .4)
