              expand.grid(Density = seq(min(Density),max(Density), length=41),
                               Variety=levels(Variety)))
lines$Yield <- predict(lm(Yield ~ Density + I(Density^2) + Variety, 
                          data = tomato),
                       lines)

g2 <- ggplot(tomato, 
             aes(
               x        = Density, 
               y        = Yield, 
